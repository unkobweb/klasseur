{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "migrate": "node ace migration:run --force=true",
    "seed": "node ace db:seed",
    "start": "node ./build/server.js",
    "deploy": "node ace migration:run --force=true && node ace db:seed && node ace build --production --ignore-ts-errors && node ./build/server.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.3.7",
    "adonis-preset-ts": "^2.1.0",
    "pino-pretty": "^7.1.0",
    "typescript": "^4.2.4",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.0.10",
    "@adonisjs/core": "^5.4.0",
    "@adonisjs/drive-s3": "^1.0.5",
    "@adonisjs/lucid": "^16.2.1",
    "@adonisjs/repl": "^3.1.7",
    "luxon": "^2.0.2",
    "mysql": "^2.18.1",
    "phc-argon2": "^1.1.2",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.20",
    "uuid": "^8.3.2"
  }
}
